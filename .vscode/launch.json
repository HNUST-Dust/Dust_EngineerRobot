{
    "version": "0.2.0",
    "configurations": [
        // 使用dap-link(如无线调试器时的参考配置)
        {
            "name": "DAPlink-launch",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/Dust_EngineerRobot.elf",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/Board/dm-h723/config/openocd_dap.cfg",
            ],
            "runToEntryPoint": "main",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "rtos": "FreeRTOS",
        },
        // 使用j-link进行调试时的参考配置
        {
            "name": "Jlink",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/Dust_EngineerRobot.elf",
            "request": "launch",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "rtos": "FreeRTOS",
        
        },
        {
            "name": "DAP-attach",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build/Dust_EngineerRobot.elf", 
            "request": "attach",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/Board/dm-h723/config/openocd_dap.cfg",
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "rtos": "FreeRTOS",
        },
        {
            "name": "Jlink-attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/build/Dust_EngineerRobot.elf",
            "request": "attach",
            "type": "cortex-debug",
            "device": "STM32H723VG",
            "showDevDebugOutput": "none",
            "servertype": "jlink",
            "interface": "swd",
            "svdFile": "${workspaceRoot}/Board/dm-h723/STM32H723.svd",
            "rtos": "FreeRTOS",
        },
    ],
}